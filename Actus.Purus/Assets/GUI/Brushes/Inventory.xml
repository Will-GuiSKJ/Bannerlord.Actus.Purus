<?xml version="1.0" encoding="utf-8" ?>
<Brushes>
  <Brush Name="Inventory.ActusPurus.UpgradeButton">
    <Layers>
      <BrushLayer Name="Default" Sprite="TroopTierIcons\icon_tier_2_big" />
    </Layers>
    <Styles>
      <Style Name="Default">
        <BrushLayer Name="Default" Sprite="TroopTierIcons\icon_tier_2_big" ColorFactor="0.7" IsHidden="false" />
      </Style>
      <Style Name="Hovered">
        <BrushLayer Name="Default" Sprite="TroopTierIcons\icon_tier_2_big" ColorFactor="1.5" IsHidden="false" />
      </Style>
      <Style Name="Pressed">
        <BrushLayer Name="Default" Sprite="TroopTierIcons\icon_tier_2_big" ColorFactor="2" IsHidden="false" />
      </Style>
      <Style Name="Disabled">
        <BrushLayer Name="Default" Sprite="TroopTierIcons\icon_tier_2_big" IsHidden="true" />
      </Style>
    </Styles>
    <SoundProperties>
      <EventSounds>
        <EventSound EventName="Click" Audio="popup" />
      </EventSounds>
    </SoundProperties>
  </Brush>
</Brushes>